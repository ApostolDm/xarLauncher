{
   "name": "xarlauncher",
   "version": "1.2.2",
   "description": "Xarium Launcher",
   "productName": "XarLauncher",
   "main": "main.js",
   "scripts": {
      "start": "electron .",
      "build": "electron-builder -w -m -l",
      "build-win": "electron-builder -w",
      "build-mac": "electron-builder -m",
      "build-linux": "electron-builder -l"
   },
   "repository": {
      "type": "git",
      "url": "https://github.com/youngive/xarLauncher"
   },
   "keywords": [
      "xar",
      "xarium",
      "xarlauncher",
      "shararam",
      "шарарам",
      "dale",
      "daisydale"
   ],
   "build": {
      "publish": [
         {
            "provider": "github",
            "owner": "youngive",
            "repo": "xarLauncher"
         }
      ],
      "appId": "cc.xarium",
      "mac": {
         "asar": false,
         "category": "public.app-category.games",
         "icon": "icons/icon.png"
      },
      "linux": {
         "asar": false,
         "target": [
            {
               "target": "deb",
               "arch": [
                  "x64",
                  "ia32"
               ]
            },
            {
               "target": "AppImage",
               "arch": [
                  "x64",
                  "ia32"
               ]
            },
            {
               "target": "pacman",
               "arch": [
                  "x64",
                  "ia32"
               ]
            }
         ],
         "icon": "icons/icon.png"
      },
      "win": {
         "asar": false,
         "target": [
            {
               "target": "nsis",
               "arch": [
                  "x64",
                  "ia32"
               ]
            }
         ],
         "icon": "icons/icon.ico"
      }
   },
   "author": "youngive <arsenij.lubimovv@gmail.com> (https://xarium.cc)",
   "license": "CC0-1.0",
   "devDependencies": {
      "electron": "11.3",
      "electron-builder": "^22.10.4",
      "electron-packager": "^17.1.1"
   },
   "dependencies": {
      "electron-updater": "^5.3.0"
   }
}
